    "eslint-plugin-react": "^7.8.2",
    "eslint-plugin-react-hooks": "^1.6.1",
    "gulp": "^4.0.2",
    "gulp-cli": "^2.3.0",
    "gulp-cssnano": "^2.1.3",
    "gulp-htmlclean": "^2.7.22",
    "gulp-imagemin": "^7.1.0",
    "gulp-uglify": "^3.0.2",
    "mobx-logger": "^0.7.1",
    "mobx-react-devtools": "^6.1.1",
    "stylelint": "9.3.0",
